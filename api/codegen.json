{
  "overwrite": true,
  "schema": "./graphql/schema.ts",
  "documents": null,
  "generates": {
    "./graphql/generated-types.ts": {
      "plugins": ["typescript", "typescript-resolvers"],
      "config": {
        "scalars": {
          "DateTime": "Date"
        }
      }
    },
    "./graphql/graphql.schema.json": { "plugins": ["introspection"] }
  }
}
